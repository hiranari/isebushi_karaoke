{
  "dart.lineLength": 120,
  "dart.showLintWarnings": true,
  "flutter.previewEmbeddedAndroidViews": true,
  "files.autoSave": "onFocusChange",
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit"
  },
  "problems.decorations.enabled": true,
  "dart.analysisExcludedFolders": [
    "tools/"
  ],
  "editor.rulers": [120],
  "dart.showTodos": false,
  "dart.showIgnoreQuickFixes": false,
  "editor.quickSuggestions": {
    "strings": true
  },
  "dart.previewFlutterUiGuides": true,
  "dart.previewFlutterUiGuidesCustomTracking": true,
  "cSpell.words": [
    "isebushi",
    "karaoke",
    "カラオケ",
    "歌唱",
    "ピッチ",
    "vibrato",
    "ビブラート",
    "centpitches",
    "detrended",
    "frameRate",
    "pitchAccuracy",
    "stabilityScore",
    "timingScore",
    "fundamentalFrequencies",
    "frequencyIndex",
    "baseFrequency",
    "harmonicFrequency",
    "camelCase",
    "PascalCase",
    "UPPER_SNAKE_CASE",
    "snake_case",
    "lowerCamelCase",
    "extractPitchFromAudio",
    "PitchDetectionService",
    "DEFAULT_SAMPLE_RATE",
    "pitch_detection_service",
    "fundamentalFreqs",
    "freqIndex",
    "baseFreq",
    "avgErr",
    "maxVal",
    "averageError",
    "maximumValue",
    "withOpacity",
    "withValues",
    "prefer_const_constructors",
    "avoid_print",
    "use_build_context_synchronously",
    "constant_identifier_names",
    "unnecessary_overrides",
    "deprecated_member_use",
    "unused_element",
    "mockResult",
    "testWidgets",
    "WidgetTester",
    "findsOneWidget",
    "findsNothing",
    "findsNWidgets",
    "pumpWidget",
    "MaterialApp",
    "Scaffold",
    "LinearProgressIndicator",
    "DateTime",
    "toStringAsFixed",
    "textContaining",
    "onShowDetails",
    "perfectResult",
    "poorResult",
    "aRankResult",
    "averageDeviation",
    "maxDeviation",
    "correctNotes",
    "totalNotes",
    "pitchPoints",
    "deviationHistory",
    "averageLatency",
    "maxLatency",
    "earlyNotes",
    "lateNotes",
    "onTimeNotes",
    "latencyHistory",
    "averageVariation",
    "maxVariation",
    "stableNotes",
    "unstableNotes",
    "variationHistory",
    "scoreBreakdown",
    "pitchAnalysis",
    "timingAnalysis",
    "stabilityAnalysis",
    "analysisService",
    "feedbackService",
    "scoringService",
    "comprehensiveScore",
    "improvementSuggestion",
    "DTW",
    "centDifferences",
    "alignedPairs",
    "alignedPitches",
    "vibratoAnalysis",
    "analyzedAt",
    "referencePitches",
    "singingPitches",
    "sampleRate",
    "frameLength",
    "pitchComparison",
    "preprocessPitches",
    "calculatePitchDistance",
    "pitchToCents",
    "calculateCentDifferences",
    "analyzePitchStability",
    "analyzeVibrato",
    "analyzeTimingAccuracy",
    "calculateOverallScore",
    "createEmptyResult",
    "compareSimple",
    "PitchComparisonException",
    "AlignedPitchPair",
    "PitchStabilityAnalysis",
    "VibratoAnalysis",
    "TimingAccuracyAnalysis",
    "PitchStabilitySegment",
    "VibratoSegment",
    "TimingDeviation",
    "ComprehensiveScore",
    "SongResult",
    "ScoreBreakdown",
    "PitchAnalysis",
    "TimingAnalysis",
    "StabilityAnalysis",
    "OverallScoreWidget"
  ],
  "cSpell.language": "en,ja",
  "cSpell.enabledFileTypes": {
    "dart": true,
    "yaml": true,
    "markdown": true,
    "json": true
  },
  "cSpell.ignorePaths": [
    "node_modules",
    "build",
    ".git",
    "*.g.dart",
    "*.freezed.dart",
    "*.mocks.dart"
  ],
  "cSpell.overrides": [
    {
      "filename": "README.md",
      "dictionaries": ["project-words"],
      "ignoreRegExpList": ["code-block-vars"]
    },
    {
      "filename": "CONTRIBUTING.md",
      "dictionaries": ["project-words"],
      "ignoreRegExpList": ["code-block-vars"]
    }
  ],
  "cSpell.dictionaries": [
    "project-words"
  ],
  "cSpell.customDictionaries": {
    "project-words": {
      "name": "project-words",
      "path": "./.vscode/project-words.txt",
      "description": "Project specific words",
      "addWords": true
    }
  },
  "cSpell.languageSettings": [
    {
      "languageId": "markdown",
      "ignoreRegExpList": ["code-block-vars"]
    }
  ],
  "cSpell.ignoreRegExpList": [
    "code-block-vars"
  ],
  "cSpell.patterns": [
    {
      "name": "code-block-vars",
      "pattern": "/(```[\\s\\S]*?```|`[^`]*`|❌|✅)/g"
    }
  ],

  // Gradle関連のエラーを無視
  "java.compile.nullAnalysis.mode": "disabled",
  "java.errors.incompleteClasspath.severity": "ignore",
  "gradle.nestedProjects": true,
  "files.exclude": {
    "**/.gradle": true
  },
  "java.configuration.updateBuildConfiguration": "interactive",
  "githubCopilot.language": "ja-JP"
}